{
    "name": "express-lines",
    "displayName": "Express-Lines",
    "description": "Perform code evaluation on selections",
    "version": "0.0.1",
    "publisher": "spywhere",
    "icon": "images/icon.png",
    "bugs": {
        "url": "https://github.com/spywhere/vscode-guides/issues"
    },
    "homepage": "https://github.com/spywhere/vscode-guides/blob/master/README.md",
    "keywords": [
        "indentation",
        "guides",
        "indentation guides",
        "ruler"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/spywhere/vscode-guides.git"
    },

    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:expressLines.performEvaluation"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [{
            "command": "expressLines.performEvaluation",
            "title": "Express-Lines: Perform code evaluation on selections"
        }]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}
